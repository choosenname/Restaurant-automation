<Window x:Class="WpfApp1.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="ManagerWindow" Height="auto" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <TextBox IsReadOnly="True" x:Name="textBox" Margin="10" VerticalScrollBarVisibility="Auto"/>
            <Button Content="Обнулить" Width="auto" Height="30" Margin="0,5,0,0" Padding="20 0" HorizontalAlignment="Center" VerticalAlignment="Top" Click="End_Click" Background="#2C3E50" Foreground="White"/>
            <Button Content="Печать" Width="auto" Height="30" Margin="0,5,0,0" Padding="20 0" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Button_Click" Background="#2C3E50" Foreground="White"/>

        </StackPanel>
    </Grid>
</Window>
